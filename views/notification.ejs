<% include ./partials/header %>

<h1>
	Notification
</h1>

<div class="container">
	<div class="row">
		<div class="col-md-12 mb-2">
			<ul>
				<% user.notifications.reverse().forEach(function(notification) { %>
					<li><%= notification.username %> just commented on your <a href="/notifications/<%= notification._id %>">post</a></li> 
				<% }) %>
			</ul>
		</div>
	</div>
</div>

<% include ./partials/footer %>